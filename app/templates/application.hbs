{{outlet}}

<div class="menu">
  {{#if session.isAuthenticated}}
    <h3>hello, {{user.name}}</h3>
    <button class="primary button" {{action 'invalidateSession'}}>Logout</button>
  {{else}}
    <button class="sign-in-with-google" {{action 'authenticate' 'google-token'}}></button>
  {{/if}}
</div>
